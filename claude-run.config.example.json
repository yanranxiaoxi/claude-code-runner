{
	"dockerImage": "claude-code-runner:latest",
	"buildImage": true,
	"dockerfile": null,
	"detached": false,
	"autoPush": true,
	"autoCreatePR": true,
	"autoStartClaude": true,
	"codeRunner": "claude",
	"defaultShell": "claude",
	"envFile": ".env",
	"environment": {
		"NODE_ENV": "development"
	},
	"setupCommands": ["npm install", "npm run build"],
	"volumes": [],
	"mounts": [
		{
			"source": "./data",
			"target": "/workspace/data",
			"readonly": false
		},
		{
			"source": "/home/user/shared-configs",
			"target": "/configs",
			"readonly": true
		}
	],
	"allowedTools": ["*"],
	"maxThinkingTokens": 100000,
	"bashTimeout": 600000,
	"containerPrefix": "claude-code-runner",
	"claudeConfigPath": "~/.claude.json",
	"codexConfigPath": "~/.codex",
	"kimiConfigPath": "~/.kimi",
	"qwenConfigPath": "~/.qwen",
	"dockerSocketPath": null,
	"forwardSshKeys": true,
	"forwardGpgKeys": true,
	"forwardSshAgent": true,
	"enableGpgSigning": false
}
